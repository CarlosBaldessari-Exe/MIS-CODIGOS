program bisiesto;
var anio : integer;
begin
write('Ingrese un an~o: ');
readln(anio);
if anio < 0 then
writeln('El an~o debe ser un numero positivo')
else if anio mod 400 = 0 then
writeln('Es bisiesto')
else if anio mod 100 = 0 then
writeln('No es bisiesto')
else if anio mod 4 = 0 then
writeln('Es bisiesto')
else
writeln('No es bisiesto')
end.